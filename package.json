{
  "name": "report-generator",
  "version": "2.0.0",
  "description": "Word document report generator",
  "main": "server.js",
  "scripts": {
    "start": "tsc & node dist/server.js",
    "build": "tsc",
    "dev": "concurrently --kill-others \"npm run watch-ts\"",
    "start-dev": "nodemon dist/server.js",
    "watch-ts": "tsc-watch -w -p ./ --onSuccess \"npm run start-dev\"",
    "lint": "tsc --noEmit && eslint '*/**/*.ts' --fix",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:coverage": "jest --coverage",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "docx",
    "template"
  ],
  "author": "Helios",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/core-js": "^2.5.4",
    "@types/dateformat": "^3.0.1",
    "@types/image-size": "^0.8.0",
    "@types/inquirer": "^6.5.0",
    "@types/mathjs": "^6.0.11",
    "@types/minimist": "^1.2.1",
    "@types/node": "^12.19.15",
    "@types/ora": "^3.2.0",
    "@types/pizzip": "^3.0.1",
    "@types/tmp": "^0.1.0",
    "@types/uuid": "^3.4.9",
    "@types/xml-js": "^1.0.0",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "aws-sdk": "^2.831.0",
    "axios": "^0.21.1",
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "core-js": "^3.8.3",
    "dateformat": "^3.0.3",
    "docxtemplater": "^3.25.4",
    "dotenv": "^8.2.0",
    "highcharts-export-server": "^2.1.0",
    "image-size": "^0.8.3",
    "inquirer": "^7.3.3",
    "jest": "^27.0.4",
    "jest-html-reporter": "^3.4.1",
    "joi": "^17.3.0",
    "jszip": "^3.7.1",
    "mathjs": "^9.4.2",
    "minimist": "^1.2.5",
    "ora": "^4.1.1",
    "pizzip": "^3.0.6",
    "request-context": "^2.0.0",
    "tmp": "^0.1.0",
    "ts-jest": "^27.0.2",
    "uuid": "^3.4.0",
    "xml-js": "^1.6.11"
  }
}
